{
  "appId": "app.eduroam.geteduroam",
  "appName": "geteduroam",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "cordova": {
    "preferences": {
      "ScrollEnabled": "false",
      "android-minSdkVersion": "19",
      "BackupWebStorage": "none",
      "SplashShowOnlyFirstTime": "false",
      "SplashScreen": "false"
    }
  },
  "plugins": {
    "SplashScreen": {
      "launchAutoHide": true,
      "showSpinner": false
    },
    "LocalNotifications": {
      "smallIcon": "ic_stat_icon_config_sample",
      "iconColor": "#488AFF",
      "sound": "beep.wav"
    }
  }
}
